    <TextView
        android:id="@+id/reactDesc"
        android:layout_width="192dp"
        android:layout_height="87dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="268dp"
        android:background="#B4000000"
        android:text="@string/ReactT1_desc"
