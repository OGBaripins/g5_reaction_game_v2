<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".testScreens.ReactTest1Activity">

    <ImageView
        android:id="@+id/bgPink"
        android:layout_width="700dp"
        android:layout_height="1200dp"
        android:layout_marginStart="-120dp"
        android:layout_marginTop="-220dp"
        android:clickable="false"
        android:contentDescription="@string/desc"
        android:onClick="goToFail"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bgpink"
        tools:ignore="DuplicateSpeakableTextCheck,DuplicateClickableBoundsCheck" />

    <ImageView
        android:id="@+id/bgBlue"
        android:layout_width="700dp"
        android:layout_height="1200dp"
        android:layout_marginStart="-1000dp"
        android:layout_marginTop="-220dp"
        android:clickable="true"
        android:contentDescription="@string/desc"
        android:onClick="goToResult"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bgblue" />

    <ImageView
        android:id="@+id/descReactT1"
        android:layout_width="268dp"
        android:layout_height="246dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="100dp"
        android:contentDescription="@string/desc"
        android:onClick="startRTest1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/descreactt1" />

</androidx.constraintlayout.widget.ConstraintLayout>